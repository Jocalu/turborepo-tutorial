{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es2016",
    "declaration": false,
    "declarationMap": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}